<div id="keyboard" class="keyboard-holder" style={{ this.octaveWidth }}>
  {{#each this.data as |key|}}
    <KeyboardKey
    @hotkey=""
    @octave={{ key.octave }}
    @note={{ key.note }}
    class={{ key.class }}
    style={{ key.style }}
    @info="{{ key.note }}{{key.octave}}"
    @pressed={{key.pressed}}
    @onPress={{action "onKeyboardPress"}}
    />
  {{/each}}
</div>
